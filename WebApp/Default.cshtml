@{
    Layout = "~/_SiteLayout.cshtml";
    Page.Title = "Home Page";
}
<form id="test" action="/" target="_blank" method="post" class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-sm-2">State:</label>
        <div class="col-sm-10">
            <input data-role="dropdownlist"
                   data-text-field="text"
                   data-value-field="value"
                   data-bind="value: state, source: states"/>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2">County:</label>
        <div class="col-sm-10">
            <input data-role="dropdownlist"
                   data-text-field="text"
                   data-value-field="value"
                   data-bind="value: county, source: counties" />
        </div>
    </div>
</form>
@section footer
{
    <script src="~/assets/js/default.js" crossorigin="anonymous"></script>
}